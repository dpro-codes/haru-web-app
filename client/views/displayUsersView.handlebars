<div>
	<header class="main-header-admin">
		<nav class="main-nav-admin nav-admin">
			<ul>
				<li><a href="/admin/coaches">Coaches</a></li>
				<li><a href="/admin/courses">Courses</a></li>
				<li><a href="/admin/users">Users</a></li>
				<li><a>
						<form action="/admin/logout?_method=DELETE" method="POST">
							<button type="submit" class="btn-logout-admin">Log out</button>
						</form>
					</a>
				</li>
			</ul>
		</nav>
	</header><br>
	<div class="table-responsive">
		<h2 class="admin-page">Users</h2>
		<table class="table table-condensed">
			{{#if data}}
			<thead>
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Email Address</th>
					<th>Password</th>
				</tr>
			</thead>
			{{else}}
			<tr></tr>
			{{/if}}
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.firstName}}</td>
						<td>{{this.lastName}}</td>
						<td>{{this.emailAddress}}</td>
						<td>{{this.password}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table><br><br>
	</div>
</div>
